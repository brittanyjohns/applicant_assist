<div class="d-flex flex-column justify-content-center align-items-center pt-5">
  <h1 class="">Welcome</h1>
  <h3>Applicant Assist</h3>
  <%= link_to "Sign Up", new_user_registration_path, class: "btn btn-lg btn-primary rounded-pill" unless user_signed_in? %>
</div>
<% if user_signed_in? %>
  <ul>
    <% current_user.orders.each do |order| %>
      <li><%= link_to order.id, order %></li>
    <% end %>
  </ul>
<% end %>
